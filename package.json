{
  "name": "vite-elm-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "test": "elm-test-rs  tests/**/*.elm",
    "test:watch": "elm-test-rs --watch tests/**/*.elm",
    "lint:ts": "tsc --noEmit",
    "lint:elm": "elm-review src",
    "fmt": "elm-format src --yes",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "elm-tooling install"
  },
  "devDependencies": {
    "elm-debug-transformer": "^1.2.1",
    "elm-review": "^2.10.3",
    "elm-tooling": "^1.15.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.0",
    "vite-plugin-elm": "^3.0.0"
  }
}
